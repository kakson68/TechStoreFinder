  <% if alert %>
    <div class="alert  alert-warning"><%= alert %></div>
  <% end %>
   <!-- Flash notice config ends here -->
      
      <%= yield %>
      </div>
    
    <%= render 'layouts/shared/footer' %>
    
  </body>
  
  
</html>
